{
  "name": "philippgrashoff/notificationforatk",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config":{
    "platform":{
      "php":"7.3"
    }
  },
  "require":{
    "atk4/data": "dev-develop",
    "atk4/schema": "dev-develop",
    "philippgrashoff/secondarymodelforatk": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": ">8",
    "atk4/ui": "dev-develop",
    "philippgrashoff/traitsforatkdata": "dev-master"
  },
  "autoload":{
    "psr-4":{
      "notificationforatk\\":"src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "notificationforatk\\tests\\": "tests"
    }
  }
}
